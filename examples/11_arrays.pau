def gimme_a_glow_up(x: [int; 5]): [int; 5]
    var result = [0, 0, 0, 0, 0];

    var idx = 0u;
    while idx < 5u do
        let value = x[idx];

        if value >= 0 then
            result[idx] := value;
        else
            result[idx] := -value;
        end

        idx := idx + 1u;
    end

    result
end

def main(): int
    let before = [0, 5, 24, -55, 98];

    let after = gimme_a_glow_up(before);

    0
end